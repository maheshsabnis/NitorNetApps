@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<a asp-action="Create">Create Role</a>

<table class="table table-bordered table-striped table-danger">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>
<hr/>
<a asp-action="AssignRoleToUser">Add USer to Role</a>